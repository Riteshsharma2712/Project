   "source": [
    "def calculate_sum(n):\n",
    "    total = 0\n",
    "    for i in range(1, n + 1):\n",
    "        total += i\n",
    "    return total\n",
    "\n",
    "n = 5\n",
    "result = calculate_sum(n)\n",
    "print(result)"
   ]
  }
